{
  "name": "firedux",
  "version": "0.1.0",
  "description": "Firebase + Redux for ReactJS",
  "repository": "adjohnson916/firedux",
  "main": "dist/index.js",
  "scripts": {
    "test": "eslint src && mocha --require babel-core/register",
    "build:test": "browserify test/index.js -o dist/test/index.js -t babelify",
    "build:test:watch": "watchify test/index.js -o dist/test/index.js -t babelify",
    "build:src": "browserify src/index.js -o dist/src/index.js -t babelify",
    "build": "npm ru build:test && npm ru build:src"
  },
  "author": "Anders D. Johnson (https://github.com/adjohnson916)",
  "license": "MIT",
  "devDependencies": {
    "arrow-mocha": "^1.0.1",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "browserify": "^12.0.1",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-plugin-standard": "^1.3.1",
    "karma": "^0.13.16",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chrome-launcher": "^0.2.2",
    "karma-cli": "^0.1.2",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.2",
    "mocha": "^2.3.4",
    "phantomjs": "^1.9.19",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.3",
    "watchify": "^3.6.1"
  },
  "dependencies": {
    "debug": "^2.2.0",
    "es6-promise": "^3.0.2",
    "firebase": "^2.3.2",
    "lodash": "^3.10.1",
    "object-assign": "^4.0.1",
    "updeep": "^0.11.0"
  }
}
